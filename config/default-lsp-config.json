{
  "servers": [
    {
      "name": "typescript-language-server",
      "command": "typescript-language-server",
      "args": ["--stdio"],
      "fileExtensions": [".ts", ".tsx", ".js", ".jsx"],
      "languageIds": ["typescript", "typescriptreact", "javascript", "javascriptreact"],
      "initializationOptions": {},
      "settings": {
        "typescript": {
          "suggest": {
            "includeCompletionsForModuleExports": true
          }
        },
        "javascript": {
          "suggest": {
            "includeCompletionsForModuleExports": true
          }
        }
      },
      "enabled": true
    },
    {
      "name": "pyright",
      "command": "pyright-langserver",
      "args": ["--stdio"],
      "fileExtensions": [".py"],
      "languageIds": ["python"],
      "initializationOptions": {},
      "settings": {
        "python": {
          "analysis": {
            "autoSearchPaths": true,
            "useLibraryCodeForTypes": true,
            "diagnosticMode": "workspace"
          }
        }
      },
      "enabled": false
    },
    {
      "name": "rust-analyzer",
      "command": "rust-analyzer",
      "args": [],
      "fileExtensions": [".rs"],
      "languageIds": ["rust"],
      "initializationOptions": {
        "checkOnSave": {
          "command": "clippy"
        }
      },
      "settings": {
        "rust-analyzer": {
          "cargo": {
            "buildScripts": {
              "enable": true
            }
          },
          "procMacro": {
            "enable": true
          }
        }
      },
      "enabled": false
    },
    {
      "name": "gopls",
      "command": "gopls",
      "args": [],
      "fileExtensions": [".go"],
      "languageIds": ["go"],
      "initializationOptions": {},
      "settings": {
        "gopls": {
          "analyses": {
            "unusedparams": true
          },
          "staticcheck": true
        }
      },
      "enabled": false
    },
    {
      "name": "clangd",
      "command": "clangd",
      "args": ["--background-index", "--clang-tidy"],
      "fileExtensions": [".c", ".cpp", ".cc", ".cxx", ".h", ".hpp"],
      "languageIds": ["c", "cpp"],
      "initializationOptions": {},
      "settings": {},
      "enabled": false
    },
    {
      "name": "jdtls",
      "command": "jdtls",
      "args": [],
      "fileExtensions": [".java"],
      "languageIds": ["java"],
      "initializationOptions": {},
      "settings": {
        "java": {
          "configuration": {
            "runtimes": []
          }
        }
      },
      "enabled": false
    },
    {
      "name": "lua-language-server",
      "command": "lua-language-server",
      "args": [],
      "fileExtensions": [".lua"],
      "languageIds": ["lua"],
      "initializationOptions": {},
      "settings": {
        "Lua": {
          "diagnostics": {
            "globals": ["vim"]
          }
        }
      },
      "enabled": false
    },
    {
      "name": "bash-language-server",
      "command": "bash-language-server",
      "args": ["start"],
      "fileExtensions": [".sh", ".bash"],
      "languageIds": ["shellscript"],
      "initializationOptions": {},
      "settings": {},
      "enabled": false
    },
    {
      "name": "json-languageserver",
      "command": "vscode-json-languageserver",
      "args": ["--stdio"],
      "fileExtensions": [".json"],
      "languageIds": ["json"],
      "initializationOptions": {},
      "settings": {
        "json": {
          "schemas": []
        }
      },
      "enabled": false
    },
    {
      "name": "yaml-language-server",
      "command": "yaml-language-server",
      "args": ["--stdio"],
      "fileExtensions": [".yaml", ".yml"],
      "languageIds": ["yaml"],
      "initializationOptions": {},
      "settings": {
        "yaml": {
          "schemas": {}
        }
      },
      "enabled": false
    }
  ],
  "defaultServer": "typescript-language-server",
  "autoDetect": true
}
